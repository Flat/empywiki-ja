{{>header}}
<div id="wiki-wrapper" class="history">
<div id="head">
  <h1><strong>{{title}}</strong></h1>
</div>
<div id="wiki-history">

  <fieldset>
  <table>
    <tbody>

      {{#versions}}
      <tr>
        <td class="author">
            <a href="javascript:void(0)">
                <span class="username">{{author}}</span>
            </a>
        </td>
        <td class="commit-name">
          <span class="time-elapsed" title="{{date_full}}">{{date}}:</span>&nbsp;
          {{message}}
          [{{id7}}]
          {{#files}}
          <br>
          <a href="{{link}}">{{file}}</a>
          {{/files}}
        </td>
      </tr>
      {{/versions}}

    </tbody>
  </table>
  </fieldset>
</div>
<div id="footer">

</div>
</div>
